<fractal fractaltype="UniLyapLike: cx(1-x)">
<imageresolution xres=1024 yres=768/>
<functiondomain>
    <topleftcorner x_min=0 c_min=0/>
    <bottomrightcorner x_max=1 c_max=4/>
</functiondomain>
<maxiteration> 10000 </maxiteration>
<initialpoint> 0.78 </initialpoint>
<orbitsdiscarded> 4000 </orbitsdiscarded>
<stability> 0.2 </stability>
</fractal>
